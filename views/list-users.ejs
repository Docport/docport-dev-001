<div class="main-content container">
  <div class="row">
    <div class="span6">
      <p>Browse the database tables below and select one to see the data stored inside.</p>
    </div>
  </div>

  <div class="row">
    <div class="span12">
      <table class="table table-hover">
        <thead>
          <tr>
            <th>User Table</th>
          </tr>
        </thead>

        <tbody>
          <% users.forEach(function(user) { %>
          <tr>
            <tb><a><%= user %></a></td>
          </tr>
          <% }) %>
        </tbody>
      </table>
    </div>
  </div>
</div>
